local function OnMythicReset()
    mythicPlusDataCache[playerGUID] = nil
end

frame:RegisterEvent("WEEKLY_REWARDS_UPDATE")
frame:SetScript("OnEvent", OnMythicReset)

local mythicPlusDataCache = {}
local function GetMythicPlusData(playerGUID)
    if mythicPlusDataCache[playerGUID] then
        return mythicPlusDataCache[playerGUID]
    end
    local data = C_PlayerInfo_GetPlayerMythicPlusRatingSummary(playerGUID)
    if not data or not data.runs then return end
    -- Process runs...
    mythicPlusDataCache[playerGUID] = data
    return data
end


